(set-logic QF_UFLIA)
(set-option :incremental true)
(set-option :produce-models true)
(declare-const x Int)
(declare-const y Int)
(declare-const z Int)
(declare-fun bvadd (Int Int) Int)
(assert (and (<= y (+ x 1))
             (> y x)))
(assert (< (bvadd x y) y))
(assert (= z (ite (< x y) x y)))
(check-sat)
(push)
(check-sat-assuming (false))
(pop)
(check-sat)
(get-value (x y z))